UserDb
  ident Text
  password Text Maybe
  UniqueUser ident
  deriving Typeable
GroupDb
  created UTCTime
  deriving Eq
  deriving Show
GroupMembershipDb
  user UserDbId
  group GroupDbId
  admin Bool
  deriving Eq
  deriving Show

SessionDb
  title Text
  created UTCTime
  begin UTCTime Maybe
  end UTCTime Maybe
  group GroupDbId
  creator UserDbId
  show ShowDbId
  deriving Eq
  deriving Show
ShowDb
  created UTCTime
  title Text
  link Text
  deriving Eq
  deriving Show
CommentDb
  created UTCTime
  session SessionDbId
  content Text
  eventTime UTCTime Maybe
  user UserDbId
  deriving Eq
  deriving Show

 -- By default this file is used in Model.hs (which is imported by Foundation.hs)
